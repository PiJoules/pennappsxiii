mixin stacked_progress_bar(category_data, expense_data, bar_colors)
	- var color_index = 0
	.progress
		each obj in category_data
			if obj && obj.percentOfAll > 0
				.progress-bar(style='width: #{obj.percentOfAll}%; background-color: #{bar_colors[color_index]}')
					span.sr-only #{obj.percentOfAll}% of Total
			- color_index += 1
	