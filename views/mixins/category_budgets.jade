mixin category_budgets(category_data, bar_colors)
	- var color_index = 0
	each obj in category_data
		if obj
			h3 #{obj.categoryName}
			.progress
				.progress-bar(style='width: #{obj.percentOfCategoryBudget}%; background-color: #{bar_colors[color_index]}')
					span.sr-only #{obj.percentOfCategoryBudget}% of Total
		- color_index += 1